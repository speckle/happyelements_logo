    <script src="js/bootstrap.min.js"></script>
    <script type="text/javascript">
    	$(function () {
    		$('#collapseOne').collapse('hide');
    		$('#collapseTwo').collapse('hide');
    		$('#collapseThree').collapse('hide');
    		$('#collapseFour').collapse('hide');
    		var one_status=0;
    		var two_status=0;
    		var three_status=0;
    		var four_status=0;
    		btn();
    		$('#collapseOne').on('show.bs.collapse', function(){
    			$('#icon1').removeClass().addClass("icon-angle-up icon-large");
    			one_status=1;
    			btn();
    		});
    		$('#collapseOne').on('hide.bs.collapse', function(){
    			$('#icon1').removeClass().addClass("icon-angle-down icon-large");
    			one_status=0;
    			btn();
    		});
    		$('#collapseTwo').on('shown.bs.collapse', function(){
    			$('#icon2').removeClass().addClass("icon-angle-up icon-large");
    			two_status=1;
    			btn();
    		});
    		$('#collapseTwo').on('hide.bs.collapse', function(){
    			$('#icon2').removeClass().addClass("icon-angle-down icon-large");
    			two_status=0;
    			btn();
    		});
    		$('#collapseThree').on('shown.bs.collapse', function(){
    			$('#icon3').removeClass().addClass("icon-angle-up icon-large");
    			three_status=1;
    			btn();
    		});
    		$('#collapseThree').on('hide.bs.collapse', function(){
    			$('#icon3').removeClass().addClass("icon-angle-down icon-large");
    			three_status=0;
    			btn();
    		});
    		$('#collapseFour').on('shown.bs.collapse', function(){
    			$('#icon4').removeClass().addClass("icon-angle-up icon-large");
    			four_status=1;
    			btn();
    		});
    		$('#collapseFour').on('hide.bs.collapse', function(){
    			$('#icon4').removeClass().addClass("icon-angle-down icon-large");
    			four_status=0;
    			btn();
    		});
    		function btn(){
    			if(one_status==1&&two_status==1&&three_status==1&&four_status==1){
    			$('#display_all').html('隐藏全部<i class="icon-angle-up"></i>');
    			$('#display_all').click(function(){
		    		$('#collapseOne').collapse('hide');
		    		$('#collapseTwo').collapse('hide');
		    		$('#collapseThree').collapse('hide');
		    		$('#collapseFour').collapse('hide');
	    		});
				}else if(one_status==0&&two_status==0&&three_status==0&&four_status==0){
					$('#display_all').html('显示全部<i class="icon-angle-down"></i>');
	    			$('#display_all').click(function(){
			    		$('#collapseOne').collapse('show');
			    		$('#collapseTwo').collapse('show');
			    		$('#collapseThree').collapse('show');
			    		$('#collapseFour').collapse('show');
		    		});
				}else return false;
    		}
    	});
    </script>
